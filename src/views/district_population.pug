extends layouts/main-layout.pug
block haeder_content
    h1(class="big-title") Districts Population 

block content
    h1(class="second-title") Population in each district
    br
    table
        tr(class="table-head")
            th District                 
            th Population     
        for dp in data
            tr
                td= dp.district
                td= dp.population                                                   
    br
