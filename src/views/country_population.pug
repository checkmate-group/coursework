extends layouts/main-layout.pug
block haeder_content
    h1(class="big-title") Countries Population 

block content
    h1(class="second-title") Population in each country
    br
    table
        tr(class="table-head")
            th Country                 
            th Population     
        for cp in data
            tr
                td= cp.country
                td= cp.population                                                   
    br
