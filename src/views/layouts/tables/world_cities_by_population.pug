table(class="table")
    thead
        tr
            th Name 
            th CountryCode               
            th District     
            th Population  
    tbody
        for city,index in data
            tr(class="rows")
                td= city.Name 
                td= city.CountryCode               
                td= city.District     
                td= city.Population
                td
                 a(href=`/edit-city/${JSON.stringify(data[index])}` class=`btn btn-primary` id=`edit-button`) 
                    | Edit