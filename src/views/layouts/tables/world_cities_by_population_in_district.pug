for district in districts
    table(class="table")
        thead
            tr
                th Name 
                th CountryCode               
                th District     
                th Population   
        tbody
            for city in citiesInDistrict
                if city.District === district
                    tr(class="rows")
                        td= city.Name 
                        td= city.CountryCode               
                        td= city.District     
                        td= city.Population