table(class="table")
    thead
        tr
            th Code 
            th Name                 
            th Continent     
            th Region
            th Population
            th Capital
            
           
    tbody
        for country,index in data
            tr(class="rows" id='row-table' role='button')
                td= country.Code 
                td= country.Name                   
                td= country.Continent                                  
                td= country.Region                                  
                td= country.Population                                  
                td= country.Capital
                td
                 a(href=`/edit/${JSON.stringify(data[index])}` class=`btn btn-primary` id=`edit-button`) 
                   | Edit  


