                nav(id="toolBar" class="flex align-center justify-space-around")
                    div(class="links flex-center")
                        div(class="link" data-action="home")
                            a(href="/")
                                li Home
                        div(class="drop-down-container")
                            div(class="link" data-action="menu")
                                a
                                    li Countries 
                            div(class="drop-down hidden" data-option="countries")
                                div(class="flex flex-dir-col")
                                    a(href="/world_countries_by_population")
                                        li Ordered by population
                                    a(href="/world_countries_by_population_in_continent")
                                        li Continent - ordered by population
                                    a(href="/world_countries_by_population_in_region")
                                        li Region - ordered by population

                        div(class="drop-down-container")
                            div(class="link" data-action="menu")
                                a
                                    li Cities 
                            div(class="drop-down hidden" data-option="cities")
                                //- div(class="arrow-top")
