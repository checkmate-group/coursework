                                    a(href="/world_cities_by_population_in_continent")
                                        li Continent - ordered by population
                                    a(href="/world_cities_by_population_in_region")
                                        li Region - ordered by population
                                    a(href="/world_cities_by_population_in_country")
                                        li Country - ordered by population
                                    a(href="/world_cities_by_population_in_district")
                                        li District - ordered by population
                        div(class="drop-down-container")
                            div(class="link" data-action="menu")
                                a
                                    li Capital Cities 
                            div(class="drop-down hidden" data-option="capital-cities")
                                div(class="flex flex-dir-col")
                                    a(href="/world_capital_cities_by_population")
                                        li Ordered by population
                                    a(href="/world_capital_cities_by_population_in_continant")
                                        li Continent - ordered by population
                                    a(href="/world_capital_cities_by_population_in_region")
                                        li Region - ordered by population
                        div(class="drop-down-container")
                            div(class="link" data-action="menu")
                                a
                                    li Population Informations 
                            div(class="drop-down hidden" data-option="capital-cities")
                                div(class="flex flex-dir-col")
                                    a(href="/world_population")
                                        li Population of the world
                                    a(href="/continent_population")
                                        li Population of a continent
                                    a(href="/region_population")
                                        li Population of a region
                                    a(href="/country_population")
                                        li Population of a country
                                    a(href="/district_population")
                                        li Population of a district
                                    a(href="/city_population")
                                        li Population of a city
                                    a(href="/population_languages")
                                        li Population Languages
